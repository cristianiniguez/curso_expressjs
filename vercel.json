{
  "name": "octolion",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@octolion-db-user",
    "DB_PASSWORD": "@octolion-db-password",
    "DB_HOST": "@octolion-db-host",
    "DB_PORT": "@octolion-db-port",
    "DB_NAME": "@octolion-db-name",
    "AUTH_ADMIN_USERNAME": "@octolion-auth-admin-username",
    "AUTH_ADMIN_PASSWORD": "@octolion-auth-admin-password",
    "AUTH_ADMIN_EMAIL": "@octolion-auth-admin-email",
    "AUTH_JWT_SECRET": "@octolion-auth-jwt-secret",
    "SENTRY_DNS": "@octolion-sentry-dns",
    "SENTRY_ID": "@octolion-sentry-id"
  }
}
